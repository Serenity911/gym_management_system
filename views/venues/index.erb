<h1>Select a venue:</h1>

<ul class = "box_of_squares">
  <% for venue in @venues %>
  <li class = "square">
      <a  class="square_main" href="/venues/<%= venue.id %>"><%= venue.name %></a>
      <div class="square_footer">
        <a  class="light_text" href="/venues/<%= venue.id %>/edit">Edit</a>
        <a  class="light_text" href="/venues/<%= venue.id %>/delete">Delete</a>
      </div>
  </li>
  <% end %>
  <li class = "square">
    <a  class="square_main" href="/venues/new">Create New Venue</a>
